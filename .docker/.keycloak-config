KEYCLOAK_DB_NAME=${KEYCLOAK_DB_NAME:-keycloak_db}
KEYCLOAK_DB_USERNAME=${KEYCLOAK_DB_USERNAME:-keycloak_user}
KEYCLOAK_DB_PASSWORD=${KEYCLOAK_DB_PASSWORD:-keycloak_password}

# Database
POSTGRES_DB=${KEYCLOAK_DB_NAME}
POSTGRES_USER=${KEYCLOAK_DB_USERNAME}
POSTGRES_PASSWORD=${KEYCLOAK_DB_PASSWORD}

# Keycloak
KC_HEALTH_ENABLED=true
KC_METRICS_ENABLED=true

KC_DB=postgres
KC_DB_URL=jdbc:postgresql://keycloak-database:5432/${KEYCLOAK_DB_NAME}
KC_DB_USERNAME=${KEYCLOAK_DB_USERNAME}
KC_DB_PASSWORD=${KEYCLOAK_DB_PASSWORD}
KEYCLOAK_ADMIN=${KEYCLOAK_ADMIN:-keycloak_admin}
KEYCLOAK_ADMIN_PASSWORD=${KEYCLOAK_ADMIN_PASSWORD:-keycloak_admin_password}
